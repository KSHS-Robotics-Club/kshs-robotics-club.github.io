---
import './styles/globals.scss'
const { title, content } = Astro.props
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<meta name="content" content={content} />
		<title> {title} </title>
	</head>
	<body>
		<main>
			<slot />
		</main>
	</body>
</html>

